{
    "name": "navigator-ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "rm -rf dist && vite build",
        "build:dev": "rm -rf dist && vite build --mode development --minify false",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "preview": "vite preview",
        "generate": "for spec in $(ls ./src/types/openapi/frontend/v1alpha1/*.swagger.json); do openapi --input $spec --output ./src/types/generated/openapi-$(basename $spec .swagger.json) --client axios; done",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "e2e": "playwright test --config=e2e/playwright.config.ts",
        "e2e:ci": "E2E_SETUP_DEMO=true playwright test --config=e2e/playwright.config.ts",
        "e2e:debug": "playwright test --config=e2e/playwright.config.ts --debug",
        "e2e:ui": "playwright test --config=e2e/playwright.config.ts --ui",
        "e2e:headed": "playwright test --config=e2e/playwright.config.ts --headed",
        "e2e:report": "playwright show-report e2e/playwright-report"
    },
    "dependencies": {
        "@catppuccin/codemirror": "1.0.1",
        "@radix-ui/react-dialog": "1.1.15",
        "@radix-ui/react-dropdown-menu": "2.1.16",
        "@radix-ui/react-select": "2.2.6",
        "@radix-ui/react-slot": "1.2.3",
        "@radix-ui/react-tabs": "1.1.13",
        "@radix-ui/react-tooltip": "1.2.8",
        "@tailwindcss/postcss": "4.1.11",
        "@tailwindcss/typography": "0.5.16",
        "@tanstack/react-query": "5.83.0",
        "@types/d3": "7.4.3",
        "@types/js-yaml": "4.0.9",
        "@uiw/codemirror-extensions-basic-setup": "4.24.1",
        "@uiw/codemirror-extensions-langs": "4.24.1",
        "@uiw/react-codemirror": "4.24.1",
        "autoprefixer": "10.4.21",
        "axios": "^1.12.1",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "d3": "7.9.0",
        "js-yaml": "4.1.0",
        "lucide-react": "0.525.0",
        "postcss": "8.5.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-router-dom": "7.6.3",
        "tailwind-merge": "3.3.1",
        "tailwindcss": "4.1.11"
    },
    "devDependencies": {
        "@eslint/js": "9.30.1",
        "@jest/globals": "^30.1.2",
        "@playwright/test": "1.55.0",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "24.0.14",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@vitejs/plugin-react": "4.6.0",
        "eslint": "9.30.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-react-refresh": "0.4.20",
        "globals": "16.3.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "openapi-typescript-codegen": "0.29.0",
        "prettier": "3.4.2",
        "ts-jest": "^29.4.1",
        "typescript": "5.8.3",
        "typescript-eslint": "8.35.1",
        "vite": "7.1.5"
    }
}
