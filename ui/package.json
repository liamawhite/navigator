{
    "name": "navigator-ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "rm -rf dist && vite build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "preview": "vite preview",
        "generate": "for spec in $(ls ./src/types/openapi/frontend/v1alpha1/*.swagger.json); do openapi --input $spec --output ./src/types/generated/openapi-$(basename $spec .swagger.json) --client axios; done"
    },
    "dependencies": {
        "@codemirror/lang-json": "^6.0.2",
        "@codemirror/lang-yaml": "^6.1.2",
        "@codemirror/theme-one-dark": "^6.1.3",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.12",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@tailwindcss/postcss": "4.1.11",
        "@tailwindcss/typography": "0.5.16",
        "@tanstack/react-query": "5.83.0",
        "@types/js-yaml": "^4.0.9",
        "@types/react-router-dom": "5.3.3",
        "@uiw/codemirror-extensions-basic-setup": "^4.24.1",
        "@uiw/codemirror-extensions-langs": "^4.24.1",
        "@uiw/react-codemirror": "^4.24.1",
        "autoprefixer": "10.4.21",
        "axios": "1.10.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "js-yaml": "^4.1.0",
        "lucide-react": "^0.525.0",
        "postcss": "8.5.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-router-dom": "7.6.3",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "4.1.11"
    },
    "devDependencies": {
        "@eslint/js": "9.30.1",
        "@types/node": "^24.0.14",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@vitejs/plugin-react": "4.6.0",
        "concurrently": "9.2.0",
        "eslint": "9.30.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-react-refresh": "0.4.20",
        "globals": "16.3.0",
        "openapi-typescript-codegen": "^0.29.0",
        "prettier": "3.4.2",
        "tw-animate-css": "^1.3.5",
        "typescript": "5.8.3",
        "typescript-eslint": "8.35.1",
        "vite": "7.0.4",
        "wait-on": "8.0.3"
    }
}
